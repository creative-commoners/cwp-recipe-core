{
    "name": "cwp/cwp-recipe-core",
    "type": "silverstripe-recipe",
    "description": "CWP core requirements recipe",
    "homepage": "https://www.cwp.govt.nz",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-core": "^4",
        "cwp/cwp-core": "dev-master",
        "silverstripe/auditor": "dev-master",
        "silverstripe/environmentcheck": "dev-master",
        "silverstripe/hybridsessions": "dev-master",
        "silverstripe/mimevalidator": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
